<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel debugId="boundElementHolder" styleName="{style.outer}">
    <div id="id">
            <div class="{style.label}">Id</div>
            <div class="{style.value}" ui:field="id"/>
        </div>
    <div id="firstname">
            <div class="{style.label}">Firstname</div>
            <div class="{style.value}" ui:field="firstname"/>
        </div>
    <div id="lastname">
            <div class="{style.label}">Lastname</div>
            <div class="{style.value}" ui:field="lastname"/>
        </div>
    <div id="email">
            <div class="{style.label}">Email</div>
            <div class="{style.value}" ui:field="email"/>
        </div>
    <div id="organization">
            <div class="{style.label}">Organization</div>
            <div class="{style.value}" ui:field="organization"/>
        </div>
    <div id="birthdate">
            <div class="{style.label}">Birthdate</div>
            <div class="{style.value}" ui:field="birthdate"/>
        </div>
    <div id="age">
            <div class="{style.label}">Age</div>
            <div class="{style.value}" ui:field="age"/>
        </div>
    <div id="created">
            <div class="{style.label}">Created</div>
            <div class="{style.value}" ui:field="created"/>
        </div>
    <div id="talks">
            <div class="{style.label}">Talks</div>
            <div class="{style.value}" ui:field="talks"/>
        </div>
    <div id="version">
            <div class="{style.label}">Version</div>
            <div class="{style.value}" ui:field="version"/>
        </div>

    <g:Button ui:field="delete">Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
